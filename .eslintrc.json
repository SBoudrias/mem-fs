{
  "extends": [
    "xo",
    "prettier",
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "parser": "@typescript-eslint/parser",
  "env": {
    "node": true
  },
  "plugins": ["prettier", "@typescript-eslint"],
  "ignorePatterns": ["dist", "node_modules"],
  "rules": {
    "prettier/prettier": "error"
  },
  "root": true
}
